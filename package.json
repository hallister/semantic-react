{
  "name": "semantic-react",
  "version": "0.0.2",
  "description": "React components for Semantic UI.",
  "main": "./lib",
  "files": [
    "dist",
    "lib",
    "radium.js"
  ],
  "scripts": {
    "start": "NODE_ENV=development && node scripts/exec.js docs:gen && node server.js",
    "start:win": "set NODE_ENV=development&& node scripts/exec.js docs:gen&& node server.js",
    "build": "set NODE_ENV=production && node scripts/exec.js build",
    "build:min": "node scripts/exec.js build:min",
    "lib": "NODE_ENV=production && babel --source-maps --ignore=__tests__ src/components/ -d lib/",
    "lib:win": "set NODE_ENV=production && babel --source-maps --ignore=__tests__ src/components/ -d lib/",
    "docs:gen": "node scripts/exec.js docs:gen",
    "test": "node scripts/exec.js test",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/karma start --auto-watch --no-single-run karma.config.js",
    "test:watch:win": "set NODE_ENV=test&& node_modules\\.bin\\karma start --auto-watch --no-single-run karma.config.js",
    "styleguide-server": "styleguidist server",
    "styleguide-build": "styleguidist build"
  },
  "keywords": [
    "react",
    "semantic",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hallister/semantic-react.git"
  },
  "author": "Justin Hall <justin@hallister.me>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.3",
    "lodash.throttle": "^4.0.1",
    "raf": "^3.2.0",
    "react-addons-shallow-compare": "^0.14.7",
    "react-event-listener": "^0.1.1",
    "react-motion-ui-pack": "^0.6.0",
    "react-onclickoutside": "^4.5.0",
    "react-portal": "^2.0.0",
    "react-prop-types": "^0.3.0"
  },
  "peerDependencies": {
    "react": ">=0.14.2",
    "react-dom": ">=0.14.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.6.4",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.1",
    "enzyme": "^2.0.0",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.1.0",
    "express": "^4.13.4",
    "glob": "^7.0.0",
    "he": "^0.5.0",
    "history": "^2.0.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "istanbul-react": "^1.1.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.4",
    "karma-jsdom-launcher": "^3.0.0",
    "karma-mocha": "^0.2.2",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "lolex": "^1.4.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-addons-transition-group": "^0.14.7",
    "react-codemirror": "^0.2.5",
    "react-docgen": "^2.7.0",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "react-styleguidist": "^2.1.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.6.2",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1"
  }
}
