{
  "name": "semantic-react",
  "version": "0.0.2",
  "description": "React components for Semantic UI.",
  "main": "dist/semantic-react.js",
  "scripts": {
    "start": "NODE_ENV=development && node scripts/exec.js docs:gen && node server.js",
    "start:win": "set NODE_ENV=development&& node scripts/exec.js docs:gen&& node server.js",
    "build": "set NODE_ENV=production && node scripts/exec.js build",
    "build:min": "node scripts/exec.js build:min",
    "docs:gen": "node scripts/exec.js docs:gen",
    "test": "node scripts/exec.js test",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/karma start --auto-watch --no-single-run karma.config.js",
    "test:watch:win": "set NODE_ENV=test&& node_modules\\.bin\\karma start --auto-watch --no-single-run karma.config.js"
  },
  "keywords": [
    "react",
    "semantic",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hallister/semantic-react.git"
  },
  "author": "Justin Hall <justin@hallister.me>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.0",
    "raf": "^3.1.0",
    "react-onclickoutside": "^4.5.0"
  },
  "peerDependencies": {
    "react": ">=0.14.2",
    "react-dom": ">=0.14.2"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "chai": "^3.3.0",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.8.0",
    "express": "^4.13.3",
    "glob": "^6.0.1",
    "he": "^0.5.0",
    "history": "^1.13.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.0",
    "istanbul-react": "^1.1.0",
    "json-loader": "^0.5.3",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-jsdom-launcher": "^1.0.0",
    "karma-mocha": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.22",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-addons-test-utils": "^0.14.2",
    "react-addons-transition-group": "^0.14.2",
    "react-codemirror": "^0.2.0",
    "react-docgen": "^2.7.0",
    "react-router": "^1.0.0-rc4",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "skin-deep": "^0.13.0",
    "uglify-js": "^2.6.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
